generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Submission {
  id           String   @id @default(cuid())
  name         String
  email        String
  businessType String?
  teamSize     String?
  painPoints   String? 
  goal         String?
  budget       String?
  createdAt    DateTime @default(now())
}
